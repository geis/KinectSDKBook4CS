<Window x:Class="TouchApplication.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="561" Width="662" MouseLeftButtonDown="Window_MouseLeftButtonDown" MouseLeftButtonUp="Window_MouseLeftButtonUp" Closing="Window_Closing">
    <Grid>
        <Image Height="480" HorizontalAlignment="Left" Margin="0" Name="CameraImage" Stretch="Fill" VerticalAlignment="Top" Width="640" />
        <Canvas Height="480" HorizontalAlignment="Left" Margin="0" Name="SelectCanvas" VerticalAlignment="Top" Width="640">
            <Rectangle Name="SelectRectangle" StrokeThickness="4" Stroke="#FFA0142F" />
            <Ellipse Name="TouchPoint" StrokeThickness="6" Stroke="#FF4600B3" />
        </Canvas>
    <Button Content="Button" Height="23" HorizontalAlignment="Left" Margin="274,487,0,0" Name="StartButton"
        VerticalAlignment="Top" Width="75" Click="StartButton_Click" />
  </Grid>
</Window>
